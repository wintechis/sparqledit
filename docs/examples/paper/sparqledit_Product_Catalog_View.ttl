@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix schema: <http://schema.org/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix spedit: <http://iis.fraunhofer.de/sparqledit/ontology#>.

_:b0 a spedit:SparqlView;
    schema:name "Product Catalog View";
    schema:description "Product catalog view for all products from department C3.";
    schema:creator "Alice A.";
    schema:dateCreated "2022-12-01T15:58:05.356Z"^^xsd:dateTime;
    spedit:queryURL "http://ux1637:3030/sparqledit-examplepaper/query"^^xsd:anyURI;
    spedit:updateURL "http://ux1637:3030/sparqledit-examplepaper/update"^^xsd:anyURI;
    spedit:query "PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\nPREFIX schema: <http://schema.org/>\n\nSELECT ?product ?name ?gtin ?weight\nWHERE {\n  ?product rdf:type schema:Product ;\n    schema:name ?name ;\n    schema:gtin ?gtin ;\n    schema:weight [\n      schema:value ?weight\n    ]\n  FILTER (lang(?name) = 'en')\n}";
    spedit:requiresBasicAuth false.